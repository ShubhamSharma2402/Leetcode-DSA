left root right
LEETCODE:- 94

recursive approach:-

class Solution {
public:
   vector<int>ans;
   void inorder(TreeNode* root){
      if(root==NULL) return ;
        inorderTraversal(root->left);
        ans.push_back(root->val);
        inorderTraversal(root->right);
   }
    vector<int> inorderTraversal(TreeNode* root) {
      inorder(root);
      return ans;
    }
};

iterative approach:-

class Solution {
public:
    vector<int> inorderTraversal(TreeNode* root) {
    vector<int> ans;
    stack<TreeNode*>st;
    TreeNode* curr=root;

    while(curr!=NULL || !st.empty()){
        while(curr!=NULL){
            st.push(curr);
            curr=curr->left;
        }
        curr=st.top();
        st.pop();
        ans.push_back(curr->val);
        curr=curr->right;
    }
    return ans;
    }
};