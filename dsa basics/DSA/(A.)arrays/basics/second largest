//  so isme tumhe 2 variables lene hai largest and second.


class Solution {
  public:
    int getSecondLargest(vector<int> &arr) {
        int n=arr.size();
       sort(arr.begin(),arr.end());
       int larg=arr[n-1];
      
       for(int i=n-2;i>=0;i--){
           if(arr[i]<larg){
             
               return arr[i];
           }
       }
       
           return -1;
       
    }
};