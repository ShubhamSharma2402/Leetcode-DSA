  

  1st approach:-

  class Solution {
  public:
    void reverseArray(vector<int> &arr) {
     reverse(arr.begin(),arr.end());
    
     
    }
};

2nd approach:-

class Solution {
  public:
    void reverseArray(vector<int> &arr) {
      int left=0;
      int right=arr.size()-1;
      while(left<right){
          swap(arr[left],arr[right]);
          left++;
          right--;
      }
    }
};